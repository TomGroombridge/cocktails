<div class='new-product'>
	<%= form_for @product, html: { multipart: true } do |f| %>
	<%= f.label :name %>
	<br>
	<%= f.text_field :name %>
	<br>
	<br>
	<%= f.label :price %>
	<br>
	<%= f.text_field :price %>
	<br>
	<br>
	<%= f.label :image %>
	<br>
	<%= f.file_field :image %>
	<br>
	<%= f.submit %>
	<% end %>
</div>