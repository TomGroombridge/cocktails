<!DOCTYPE html>
<html>
<head>
  <title>Cocktails</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>


<div class='header'>
		<div class='logo-bar	'>

		</div>
	<!-- navbar start -->
		<div class='subscribe-bar'>
			<!-- <div class='subscribe-nav'> -->
				<div class='subscribe-links'>
					<% if user_signed_in? %>
					  <li>
					  	<%= link_to('Logout', destroy_user_session_path, :method => :delete, class: "link-decoration") %>        
					  </li>
				   	<li>
				  		<%= link_to('Edit registration', edit_user_registration_path, class: "link-decoration") %>
				 		</li>
					<% else %>
					  <li>
					  	<%= link_to('Register', new_user_registration_path, class: "link-decoration")  %>
					  </li>
					<% end %>			
				</div>
			<!-- </div> -->
		</div>
		<div class='nav-bar'>

				<a href="/about" class="home-nav">
					Home
				</a>


			<a href="/products" class="products-nav">
			
				Products
			</a>

			<div class="about-nav">
				About us
			</div>

			<a href="/pictures" class="gallery-nav">
				Gallery
			</a>

			<a href="/contacts/new" class="contact-nav">
				Contact
			</a>
		</div>
		<!-- navbar end -->



	</div>

<nav></nav>

<%= yield %>

</body>
</html>
